parameters:
  dynamicConstantNames:
    - COOKIEPATH
    - SCRIPT_DEBUG
    - SITECOOKIEPATH
    - WPCOM_IS_VIP_ENV
  scanFiles:
    - stubs/lipe.php
    - stubs/wp.php
  checkGenericClassInNonGenericObjectType: false
  ignoreErrors:
    - '#^Function apply_filters invoked with [3456789] parameters, 2 required\.$#'
    - '#^Unsafe usage of new static#'
  reportUnmatchedIgnoredErrors: false
