services:
  - class: Lipe\Lib\Phpstan\Services\GetPostTypesDynamicFunctionReturnTypeExtension
    tags:
      - phpstan.broker.dynamicFunctionReturnTypeExtension

parameters:
  dynamicConstantNames:
    - COOKIEPATH
    - DB_NAME
    - DOMAIN_CURRENT_SITE
    - SCRIPT_DEBUG
    - SITECOOKIEPATH
    - WPCOM_IS_VIP_ENV
  scanFiles:
    - stubs/lipe.php
    - stubs/wp.php
  checkGenericClassInNonGenericObjectType: false
